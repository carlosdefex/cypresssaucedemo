{
    "name": "com-bf-cypress-template",
    "version": "1.0.0",
    "license": "MIT",
    "devDependencies": {
        "cypress": "^13.10.0",
        "eslint": "^8.57.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-plugin-cypress": "^2.15.2",
        "globals": "^15.6.0",
        "husky": "^9.0.11",
        "prettier": "^3.2.5",
        "prettier-plugin-jsdoc": "^1.3.0",
        "sonarqube-scanner": "^4.0.1",
        "stylelint": "^16.6.0",
        "stylelint-config-idiomatic-order": "^10.0.0",
        "stylelint-config-recommended": "^14.0.0"
    },
    "scripts": {
        "start": "cypress open",
        "test": "cypress run",
        "cypress:ci": "cypress run --record --key 0f1e7b3b-0b3b-4b3b-8b3b-0b3b3b3b3b3b",
        "cypress:verify": "cypress run --record --key 0f1e7b3b-0b3b-4b3b-8b3b-0b3b3b3b3b3b --parallel --ci-build-id $CIRCLE_WORKFLOW_ID",
        "cypress:open:ci": "cypress run --record --key 0f1e7b3b-0b3b-4b3b-8b3b-0b3b3b3b3b3b --parallel --ci-build-id $CIRCLE_WORKFLOW_ID",
        "cypress:run:ci": "cypress run --record --key 0f1e7b3b-0b3b-4b3b-8b3b-0b3b3b3b3b3b --parallel --ci-build-id $CIRCLE_WORKFLOW_ID",
        "cypress:ci:circle": "cypress run --record --key 0f1e7b3b-0b3b-4b3b-8b3b-0b3b3b3b3b3b --parallel --ci-build-id $CIRCLE_WORKFLOW_ID",
        "cypress:ci:github": "cypress run --record --key 0f1e7b3b-0b3b-4b3b-8b3b-0b3b3b3b3b3b --parallel --ci-build-id $GITHUB_RUN_ID",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lintFix:js": "eslint . --ext .js,.jsx --fix",
        "precommit": "npm run lintFixAll",
        "prepush": "npm run lintFixAll && npm run test",
        "postinstall": "husky init",
        "prepare": "husky",
        "sonar-unix": "node sonar-project.js",
        "sonar-win": "node .\\sonar-project.js"
    }
}
